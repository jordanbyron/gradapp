<h1>New application_requirement</h1>

<%= error_messages_for :application_requirement %>

<% form_for(@application_requirement) do |f| %>
  <p>
    <b>Department</b><br />
    <%= f.text_field :department_id %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_field :description %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', application_requirements_path %>
