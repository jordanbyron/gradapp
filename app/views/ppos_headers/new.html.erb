<h1>New ppos_header</h1>

<%= error_messages_for :ppos_header %>

<% form_for(@ppos_header) do |f| %>
  <p>
    <b>Ppos</b><br />
    <%= f.text_field :ppos_id %>
  </p>

  <p>
    <b>Header</b><br />
    <%= f.text_field :header %>
  </p>

  <p>
    <b>Position</b><br />
    <%= f.text_field :position %>
  </p>

  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', ppos_headers_path %>
