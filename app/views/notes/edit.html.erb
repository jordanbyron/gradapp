<h1>Editing Note</h1>

<%= error_messages_for :header_template %>

<% form_for :note, @note, :url => graduate_applicant_note_path(@graduate_applicant), :html => { :method => :put} do |f| %>
  <p>
    <b>Note Text</b><br />
    <%= f.text_area(:text, :size => "100%x10") %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Cancel', graduate_applicant_notes_path(@graduate_applicant) %>
