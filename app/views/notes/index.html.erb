<h1>Listing notes</h1>

<table>
  <tr>
    <th>Graduate applicant</th>
    <th>Text</th>
    <th>Created date time</th>
  </tr>

<% for note in @notes %>
  <tr>
    <td><%=h note.graduate_applicant_id %></td>
    <td><%=h note.text %></td>
    <td><%=h note.created_date_time %></td>
    <td><%= link_to 'Show', note %></td>
    <td><%= link_to 'Edit', edit_note_path(note) %></td>
    <td><%= link_to 'Destroy', note, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New note', new_note_path %>
