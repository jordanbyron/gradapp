<h1>Editing attachments</h1>

<%= error_messages_for :attachments %>

<% form_for(@attachments) do |f| %>
  <p>
    <b>Gradaute applicant</b><br />
    <%= f.text_field :gradaute_applicant_id %>
  </p>

  <p>
    <b>Title</b><br />
    <%= f.text_field :title %>
  </p>

  <p>
    <b>Description</b><br />
    <%= f.text_area :description %>
  </p>

  <p>
    <b>Created by</b><br />
    <%= f.text_field :created_by %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @attachments %> |
<%= link_to 'Back', attachments_path %>
